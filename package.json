{
  "name": "marketplace",
  "version": "1.0.0",
  "description": "Marketplace",
  "author": "Konrad Gadzinowski <kgadzinowski@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "nodemon --exec \"ts-node\" -- \"src/server/BootDev.ts\"",
    "start-prod": "ts-node ./src/server/BootProd.ts",
    "test-all": "npm run test-server && npm run test-client",
    "test-client": "karma start karma.conf.js",
    "test-server": "mocha -r ts-node/register test/server/**/*.ts",
    "format-code": "prettier --print-width 200 --write '{src,test}/**/*.ts'"
  },
  "dependencies": {
    "bcrypt-nodejs": "^0.0.3",
    "bluebird": "^3.4.7",
    "body-parser": "^1.15.2",
    "chalk": "^1.1.3",
    "compression": "^1.6.2",
    "connect-mongo": "^1.3.2",
    "cookie": "^0.3.1",
    "cookie-parser": "^1.4.3",
    "dateformat": "^2.0.0",
    "dotenv": "^4.0.0",
    "errorhandler": "^1.4.3",
    "express": "^4.14.0",
    "express-flash": "^0.0.2",
    "express-pjax": "git://github.com/optikfluffel/express-pjax.git",
    "express-session": "^1.14.2",
    "express-status-monitor": "^0.1.5",
    "express-validator": "^3.1.2",
    "hbs": "^4.0.4",
    "html-entities": "^1.2.1",
    "lusca": "^1.4.1",
    "mime": "^2.2.0",
    "moment-timezone": "^0.5.13",
    "mongoose": "^4.6.6",
    "morgan": "^1.7.0",
    "multer": "^1.3.0",
    "node-sass-middleware": "^0.11.0",
    "nodemailer": "^3.1.7",
    "passport": "0.3.2",
    "passport-local": "^1.0.0",
    "passport-oauth": "^1.0.0",
    "request": "^2.78.0",
    "stripe": "^4.12.0",
    "swag": "^0.7.0",
    "typescript": "^2.3.0",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@types/chai": "^3.5.0",
    "@types/express": "^4.0.35",
    "@types/jquery": "^2.0.41",
    "@types/mocha": "^2.2.40",
    "@types/node": "^7.0.12",
    "@types/supertest": "^2.0.2",
    "chai": "^3.5.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-plugin-chai-friendly": "^0.2.0",
    "eslint-plugin-import": "^2.1.0",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "gulp-clean-css": "2.0.6",
    "gulp-concat": "^2.6.1",
    "gulp-concat-css": "^2.3.0",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.4",
    "gulp-run-sequence": "^0.3.2",
    "gulp-scss": "^1.4.0",
    "gulp-sourcemaps": "^2.6.0",
    "gulp-typescript": "^3.1.6",
    "gulp-uglifyes": "^0.1.2",
    "gulp-watch": "^4.3.11",
    "jasmine-core": "^2.5.2",
    "karma": "^1.6.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-coverage": "^1.1.1",
    "karma-htmlfile-reporter": "^0.3.5",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-jquery": "^0.1.1",
    "karma-jquery": "^0.2.0",
    "karma-junit-reporter": "^1.2.0",
    "karma-spec-reporter": "0.0.31",
    "mocha": "^3.2.0",
    "mockgoose": "^7.3.3",
    "nodemon": "^1.19.1",
    "prettier": "1.18.2",
    "sinon": "^2.0.0",
    "sinon-mongoose": "^1.3.0",
    "supertest": "^3.0.0",
    "ts-node": "^3.0.2"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "rules": {
      "comma-dangle": 0,
      "consistent-return": 0,
      "no-param-reassign": 0,
      "no-underscore-dangle": 0,
      "no-shadow": 0,
      "no-console": 0,
      "no-plusplus": 0,
      "no-unused-expressions": 0,
      "chai-friendly/no-unused-expressions": 2
    },
    "env": {
      "jasmine": true,
      "mocha": true,
      "node": true
    },
    "plugins": [
      "chai-friendly"
    ]
  }
}
